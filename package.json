{
  "name": "cyf-forms",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "REACT_APP_NODE_ENV=LOCAL react-app-rewired start",
    "start:test": "cross-env REACT_APP_NODE_ENV=TESTING react-app-rewired start",
    "build": "react-app-rewired build",
    "build:staging": "INLINE_RUNTIME_CHUNK=false REACT_APP_NODE_ENV=STAGING react-app-rewired build && mv build staging",
    "build:production": "INLINE_RUNTIME_CHUNK=false REACT_APP_NODE_ENV=PRODUCTION GENERATE_SOURCEMAP=false react-app-rewired build && mv build production",
    "test": "react-app-rewired test",
    "e2e": "concurrently -k -s first -n \"app,mock,e2e\" \"yarn start:test\" \"yarn e2e:mock\" \"yarn e2e:run\"",
    "e2e:dev": "concurrently -k -s first -n \"app,mock,e2e\" \"yarn start:test\" \"yarn e2e:mock\" \"yarn e2e:open\"",
    "e2e:mock": "node ./e2e/fixtures/server.js",
    "e2e:run": "wait-on -l http-get://localhost:3000 && cypress run",
    "e2e:open": "wait-on -l http-get://localhost:3000 && cypress open",
    "lint": "eslint .",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "cypress": "^3.3.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint-plugin-cypress": "^2.2.1",
    "express": "^4.17.1",
    "husky": "^3.0.4",
    "react-app-rewired": "^2.1.1",
    "wait-on": "^3.2.0",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
